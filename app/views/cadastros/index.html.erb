<p id="notice"><%= notice %></p>

<h1>Cadastros</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Endereço</th>
      <th>E-mail</th>
      <th>Telefone</th>
      <th>Cargo</th>
      <th>Pretenção salarial</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cadastros.each do |cadastro| %>
      <tr>
        <td><%= cadastro.name %></td>
        <td><%= cadastro.address %></td>
        <td><%= cadastro.email %></td>
        <td><%= cadastro.telefone %></td>
        <td><%= cadastro.cargo %></td>
        <td><%= cadastro.pretencao_salarial %></td>
        <td><%= link_to 'Ver', cadastro %></td>
        <td><%= link_to 'Editar', edit_cadastro_path(cadastro) %></td>
        <td><%= link_to 'Excluir', cadastro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo cadastro', new_cadastro_path %>
